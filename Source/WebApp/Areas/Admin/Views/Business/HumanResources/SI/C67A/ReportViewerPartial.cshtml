@using Eagle.WebApp.Areas.Admin.Reports.SI.C66A


@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer";
    settings.Report = (C67AXtraReport)ViewData["Report"];   
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Controller = "C67A", Action = "CallbackReportViewerPartial", FullName = ViewBag.FullName, MonthYear = ViewBag.MonthYear, Stage = ViewBag.Stage, LSCompanyID = ViewBag.LSCompanyID };
    settings.ExportRouteValues = new { Controller = "C67A", Action = "ExportReportViewerPartial", FullName = ViewBag.FullName, MonthYear = ViewBag.MonthYear, Stage = ViewBag.Stage, LSCompanyID = ViewBag.LSCompanyID };
    
}).GetHtml()
