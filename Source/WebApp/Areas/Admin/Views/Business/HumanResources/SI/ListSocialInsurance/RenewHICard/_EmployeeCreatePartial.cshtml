@model EmployeeSearchViewModel
<div class="modal hide fade popupEmpLoyee" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="min-width:245px">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 id="myModalLabel">@Eagle.Resource.LanguageResource.StaffSearch</h3>
    </div>
    <div class="modal-body">
        @Html.Partial("../Business/HumanResources/SecurityInsurance/ListSocialInsurance/RenewHICard/_EmployeeCreateFormInnerPartial")
        <div id="popEmployeeResult">

        </div>
    </div>
    <div class="modal-footer">
        <input type="button" id="btnEmployeeCancel" class="btn" data-dismiss="modal" aria-hidden="true" value="@Eagle.Resource.LanguageResource.Cancel" />
        <input type="button" id="btnEmployeeSearch" class="btn btn-info" value="@Eagle.Resource.LanguageResource.Search" />
        <input type="button" id="btnEmployeeSubmit" class="btn btn-info add AddModel" value="@Eagle.Resource.LanguageResource.Add" />
    </div>
</div>
