            </div>
            <div class="col-md-2">
                @Html.LabelFor(model => model.TLSDistrictID)
            </div>
            <div class="col-md-4">
                @Html.TextBox("SelectTLSDistrictID",null, new { @class = "span12" })
                @Html.HiddenFor(model => model.TLSDistrictName)
                @Html.HiddenFor(model => model.TLSDistrictID)

            </div>
        </div>
        <!--end row-->
        <!--start row-->
        <div class="row">
            <div class="col-md-2">
                @Html.LabelFor(model => model.TAddress)
            </div>
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.TAddress, new {  @class = "span12" })

            </div>
        </div>
        <!--end row-->
    </div>
</div>