@{
    ViewBag.Title = Eagle.Resource.LanguageResource.FunctionName_IndividualOT;
    Layout = Eagle.Repository.UI.Layout.LayoutType.MainLayout;
}
@section ContentSection
{
<form id="frmAdd" class="form-horizontal user-group-management">
<div id="alertMessageBox">
</div>
    <div id="divHeader">
        @Html.Action("_SearchHeaderEmp","IndividualOT",null)
    </div>
    <hr />
    <!--/row-->
    <div class="row">
        <div class="footbar">
            <input type="button" id="btnSearch" value="@Eagle.Resource.LanguageResource.btnSearch" class="btn" />
        </div>
    </div>
    <!--End.row-->
    <div id="divContent">
        @Html.Action("_List","IndividualOT",null)
    </div>
</form>
    }
<script type="text/javascript">
    $('#changeSidebarPos').trigger('click');
    $(document).on("click", "#btnSearch", function () {
        $("body").addClass("loading");
        //Search danh sach nhan vien 
        var moduleId = 9;
        var url = areas + '/IndividualOT/_Search?ModuleID=' + moduleId;
        $.ajax({
            type: "POST",
            url: url,
            data: $("#frmAdd").serialize(),
            success: function (data) {
                $('#divContent').html(data);
            }
        });
        
    });
</script>
