@Html.DevExpress().ReportViewer(settings =>
{
    settings.Name = "reportViewer1";
    settings.Report = (WorkingSchedule31XtraReport)ViewData["Report"];
    settings.AutoSize = false;
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Height = 1000;    
    settings.Attributes.Add("class", "reportViewer");
    settings.CallbackRouteValues = new { Controller = "WorkingSchedule", Action = "LoadWorkingScheduleReportViewer", Month = ViewBag.Month, Year = ViewBag.Year, LSCompanyID = ViewBag.LSCompanyID, FullName = ViewBag.FullName, LSPositionID = ViewBag.LSPositionID };
    settings.ExportRouteValues = new { Controller = "WorkingSchedule", Action = "ExportWorkingScheduleData", Month = ViewBag.Month, Year = ViewBag.Year, LSCompanyID = ViewBag.LSCompanyID, FullName = ViewBag.FullName, LSPositionID = ViewBag.LSPositionID };
}).GetHtml()
