@model IEnumerable<Eagle.Model.HR.QualificationViewModel>
<div class="table datatable">
    <table id="dataTable" class="use-dataTable table table-bordered table-condensed table-hover table-striped tablesorter sortableTable">
        <thead>
            <tr>
                <th class="text_center">@Eagle.Resource.LanguageResource.No</th>   
                <th>
                    @Html.DisplayNameFor(model => model.QualificationNo)
                </th>               
                <th>
                    @Html.DisplayNameFor(model => model.EmpName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FromMonth)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ToMonth)
                </th>                            
                <th>
                    @Html.DisplayNameFor(model => model.Priority)
                </th>                            
                <th class="table-actions text_center no-sort">@Eagle.Resource.LanguageResource.Actions</th>
            </tr>
        </thead>
        <tbody>
            @{int i = 0;}
            @foreach (var item in Model)
            {
                i++;
                <tr id="@item.QualificationID">
                    <td>
                        @i
                    </td>                  
                    <td>
                        @Html.DisplayFor(modelItem => item.QualificationNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EmpName)
                    </td>
                   <td class="center">
                        @Html.DisplayFor(modelItem => item.FromMonth)
                    </td>
                    <td class="center">
                        @Html.DisplayFor(modelItem => item.ToMonth)
                    </td>                                      
                   <td class="center">
                        @Html.DisplayFor(modelItem => item.Priority)
                    </td>                  
                    <td class="table-actions text_center center no-sort">
                        <a data-id="@item.QualificationID" title="Edit" class="ico editItem ">&nbsp;&nbsp;&nbsp;</a>
                        <a data-id="@item.QualificationID" title="Delete" class="ico deleteItem ">&nbsp;&nbsp;&nbsp;</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>