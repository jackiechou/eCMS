@Html.DevExpress().ReportViewer(settings =>{
    // The following settings are necessary for a Report Viewer.
    settings.Name = "reportViewerQuater";
    settings.Report = (QuarterlyCAFXtraReport)ViewData["QuarterReport"];
    settings.AutoSize = true;
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Height = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Attributes.Add("class", "reportViewer");  
    // Callback and export route values specify the corresponding controllers and their actions.
    // These settings are necessary as well.
    settings.CallbackRouteValues = new { Controller = "CAFR", Action = "PopulateReportViewer_CAFRByQuarter", Year = (ViewBag.Year != null) ? ViewBag.Year : DateTime.Now.Year, LSCompanyID = ViewBag.LSCompanyID, LanguageId = Session["LanguageId"].ToString() };
    settings.ExportRouteValues = new { Controller = "CAFR", Action = "ExportData_CAFRByQuarter", Year = (ViewBag.Year != null) ? ViewBag.Year : DateTime.Now.Year, LSCompanyID = ViewBag.LSCompanyID, LanguageId = Session["LanguageId"].ToString() };  
}).GetHtml()
