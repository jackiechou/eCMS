@model IEnumerable<Eagle.Model.HR.WorkingHistoryViewModel>
<div class="table datatable">
    <table id="dataTable" class="use-dataTable table table-bordered table-condensed table-hover table-striped tablesorter sortableTable">
        <thead>
            <tr>
                <th class="text_center">@Eagle.Resource.LanguageResource.No</th>
               @* <th>
                    @Html.DisplayNameFor(model => model.EmpName)
                </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.WorkFor)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Position)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Duty)
                </th>
                <th class="table-actions text_center no-sort">@Eagle.Resource.LanguageResource.Actions</th>
            </tr>
        </thead>
        <tbody>
            @{int i = 0;}
            @foreach (var item in Model)
            {
                i++;
                <tr>
                    <td>
                        @i
                    </td>
                  @*  <td>
                        @Html.DisplayFor(modelItem => item.EmpName)
                    </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.WorkFor)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Position)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Duty)
                    </td>
                    <td class="table-actions text_center center no-sort">
                        <a data-id="@item.WorkingHistoryID" title="Edit" class="ico editItem">&nbsp;&nbsp;&nbsp;</a>
                        <a data-id="@item.WorkingHistoryID" title="Delete" class="ico deleteItem">&nbsp;&nbsp;&nbsp;</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
