@using (Html.BeginForm(null, null, FormMethod.Post, new { enctype = "multipart/form-data", id = "MenuForm", name = "MenuForm", autocomplete = "off", @class = "form-horizontal" }))
{
    <div id="content_box" class="row">
        <!-- START LEFT FRAME  -->
        <div id="left_box" class="left-box col-md-4">
            <div id="divMenuList">
                @Html.Partial("../Sys/Menu/_MenuTree")
            </div>
            <div class="clearboth"></div>
        </div>
        <div id="right_box" class="right-box col-md-8">
            <div id="divEdit">
                @Html.Action("Create", "Menu", null)
            </div>
        </div>
    </div>
}
