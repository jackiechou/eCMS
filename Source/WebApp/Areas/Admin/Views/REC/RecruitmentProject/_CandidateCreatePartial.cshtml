@model CandidateSearchViewModel


<div class="modal hide fade popupCandidate" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="min-width:245px">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 id="myModalLabel">@Eagle.Resource.LanguageResource.Candidate</h3>
    </div>
    <div class="modal-body">
        @Html.Partial("../REC/RecruitmentProject/_CandidateCreateFormInnerPartial")
        <div id="popCandidateResult">

        </div>
    </div>
    <div class="modal-footer">
        <input type="button" id="btnCandidateCancel" class="btn" data-dismiss="modal" aria-hidden="true" value="@Eagle.Resource.LanguageResource.Cancel" />
        <input type="button" id="btnCandidateSearch" class="btn btn-info" value="@Eagle.Resource.LanguageResource.Search" />
        <input type="button" id="btnCandidateSubmit" class="btn btn-info AddModel" value="@Eagle.Resource.LanguageResource.Add" />
    </div>
</div>
