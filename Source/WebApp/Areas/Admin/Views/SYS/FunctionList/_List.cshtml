@model List<FunctionListViewModel>
<!--start box-->
<div class="box form-search">
    <div class="row">
        <div id="right_accordion" class="accordion-group">
            <div class="accordion-heading toolbar">
                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#form_accordion">
                    <span class="pull-right"><i class="icon-chevron-up icon-white"></i></span>
                    <span class="heading-tite">@Eagle.Resource.LanguageResource.Search</span>
                </a>
            </div>
            <div id="form_accordion" class="accordion-body collapse in">
                <div class="accordion-inner">
                    <div class="accordion-content">
                        <!--start row-->
                        <div class="row">
                            <div class="col-md-2">
                                <label for="ModuleID">@Eagle.Resource.LanguageResource.Group</label>
                            </div>
                            <div class="col-md-4">
                                @Html.DropDownListFor(model => model.ModuleID, null, new { @class = "span12" })
                            </div>
                            <div class="col-md-2">
                                <label for="Parent">@Eagle.Resource.LanguageResource.Parent</label>
                            </div>
                            <div class="col-md-4">
                                @Html.DropDownList("Parent", null, new { @class = "span12" })
                            </div>
                        </div>
                        <!--end row-->
                        <!--start row-->
                        <div class="row">
                            <div class="col-md-2">
                                <label for="Title">@Eagle.Resource.LanguageResource.Title</label>
                            </div>
                            <div class="col-md-10">
                                @Html.TextBoxFor(model => model.Title, null, new { @class = "span12" })
                            </div>
                        </div>
                        <!--end row-->
                        <!--start row-->
                        <div class="row">
                            <div class="col-md-2">
                                <label for="FunctionName">@Eagle.Resource.LanguageResource.FunctionName</label>
                            </div>
                            <div class="col-md-10">
                                @Html.TextBoxFor(model => model.FunctionName, null, new { @class = "span12" })
                            </div>
                        </div>
                        <!--end row-->
                    </div>
                </div>
                <!--/row-->
                <div class="row">
                    <div class="footbar">
                        <div>
                            <button class="btn" type="button" id="btnReset">@Eagle.Resource.LanguageResource.Reset</button>
                            <button class="btn btn-info" type="button" id="btnSearch">@Eagle.Resource.LanguageResource.Search</button>

                        </div>
                    </div>
                </div>
                <!--End.row-->
            </div>
        </div>
    </div>
</div>

<div id='jqxWidget'>
    <div class="grid_box" id="jqxgrid"></div>
</div>
<!--end box-->
