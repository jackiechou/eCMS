@model Eagle.Model.PayrollParameterViewModel

<form id="frmAdd" class="form-horizontal"> 
    @Html.HiddenFor(model => model.ID)    
     <div id="alertMessageBox">
        @if (ViewBag.DisplayErrorMessage == true)
        {
            <div class="@ViewBag.CssClass">
                <a href="#" class="close" data-dismiss="alert">&times;</a>
                <strong>@ViewBag.SortMessage !</strong>
                <div class="padding-left">
                    @Html.Raw(Server.HtmlDecode(ViewBag.Message))
                </div>
            </div>
        }
    </div>
    <div class="box">
        <div class="row">            
            <div id="right_accordion" class="accordion-group">
                 <div class="accordion-heading toolbar">
                    <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#form_accordion">
                        <span class="pull-right"><i class="glyphicon glyphicon-chevron-up icon-white"></i></span>
                        <span class="heading-tite">@Eagle.Resource.LanguageResource.InputArea</span>
                    </a>
                </div>
                <div id="form_accordion" class="accordion-body collapse in">
                     <div class="accordion-inner">
                            <div class="accordion-content">
                 
                            <div class="row">
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.EffectiveDate)
                                </div>
                                <div class="col-md-3">
                                        @Html.DatepickerFor(model => model.EffectiveDate)
                                        @Html.ValidationMessageFor(model => model.EffectiveDatetmp)                                 
                                </div>
                                <div class="col-md-3">
                                </div>
                                <div class="col-md-3">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.CoefInsS_E)                                      
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.CoefInsS_E, new { @class = "form-control" }) %
                                        @Html.ValidationMessageFor(model => model.CoefInsS_E) 
                                </div>
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.CoefInsS_C)
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.CoefInsS_C, new { @class = "form-control" }) %
                                        @Html.ValidationMessageFor(model => model.CoefInsS_C) 
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.CoefInsH_E)                                      
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.CoefInsH_E, new { @class = "form-control" }) %
                                        @Html.ValidationMessageFor(model => model.CoefInsH_E) 
                                </div>
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.CoefInsH_C)
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.CoefInsH_C, new { @class = "form-control" }) %
                                        @Html.ValidationMessageFor(model => model.CoefInsH_C) 
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.CoefInsE_E)                                      
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.CoefInsE_E, new { @class = "form-control" }) %
                                        @Html.ValidationMessageFor(model => model.CoefInsE_E) 
                                </div>
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.CoefInsE_C)
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.CoefInsE_C, new { @class = "form-control" }) %
                                        @Html.ValidationMessageFor(model => model.CoefInsE_C) 
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.MinSalary)                                      
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.MinSalary, "{0:###.##}", new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.MinSalary) 
                                </div>
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.CoefOver)
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.CoefOver, "{0:###.##}", new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.CoefOver) 
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.SelfDeduction)                                      
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.SelfDeduction, "{0:###.##}", new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.SelfDeduction) 
                                </div>
                                <div class="col-md-3">
                                        @Html.RequiredLabelFor(model => model.DependDeduction)
                                </div>
                                <div class="col-md-3">
                                        @Html.TextBoxFor(model => model.DependDeduction, "{0:###.##}", new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.DependDeduction) 
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                        @Html.LabelFor(model => model.Description)                                      
                                </div>
                                <div class="col-md-9">
                                        @Html.TextAreaFor(model => model.Description, new { @class = "form-control", @maxlength = "200" })
                                </div>
                            </div>
                            <!--/row-->
                            <div class="row">
                                <div class="footbar">
                                    <input type="button" id="btnReset" value="@Eagle.Resource.LanguageResource.btnReset" class="btn btn-default reset" />
                                    <input type="button" id="btnAdd" value="@Eagle.Resource.LanguageResource.btnSave" class="btn btn-info add AddModel" />
                                </div>
                            </div>
                            <!--End.row-->
                        </div>
                     </div>
                </div>
            </div>
        </div>
    </div>
</form>


<script>
    InvokeDateTimePicker();
</script>

