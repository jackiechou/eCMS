@model SYS_tblProcessOnlineMaster

<form id="frmAdd" class="form-horizontal">
    @Html.HiddenFor(model => model.OnlineProcessID)
    <div id="alertMessageBox">
        @if (ViewBag.DisplayErrorMessage == true)
        {
            <div class="@ViewBag.CssClass">
                <a href="#" class="close" data-dismiss="alert">&times;</a>
                <strong>@ViewBag.SortMessage !</strong>
                <div class="padding-left">
                    @Html.Raw(Server.HtmlDecode(ViewBag.Message))
                </div>
            </div>
        }
    </div>
    <div class="box">
        <div class="row">
            <div id="right_accordion" class="accordion-group">
                <div class="accordion-heading toolbar">
                    <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#form_accordion">
                        <span class="pull-right"><i class="icon-chevron-up icon-white"></i></span>
                        <span class="heading-tite">@Eagle.Resource.LanguageResource.InputArea</span>
                    </a>
                </div>
                <div id="form_accordion" class="accordion-body collapse in">
                    <div class="accordion-inner">
                        <div class="accordion-content">

                            <div class="row">
                                <div class="col-md-2">
                                    @Html.RequiredLabelFor(model => model.DMQuiTrinhID)
                                </div>
                                <div class="col-md-4">
                                    @Html.DropDownList("DMQuiTrinhID", null, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.DMQuiTrinhID)
                                </div>
                                <div class="col-md-2">
                                </div>
                                <div class="col-md-4">
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel1A, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel1A, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel1A)
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel1U, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel1U, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel1U)
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel2A, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel2A, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel2A)
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel2U, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel2U, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel2U)
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel3A, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel3A, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel3A)
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel3U, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel3U, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel3U)
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel4A, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel4A, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel4A)
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel4U, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel4U, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel4U)
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel5A, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel5A, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel5A)
                                </div>
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.StatusLevel5U, new { @class="reset-margin reset-padding" })
                                </div>
                                <div class="col-md-4">
                                    @Html.TextBoxFor(model => model.StatusLevel5U, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.StatusLevel5U)
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2">
                                    @Html.LabelFor(model => model.Description)
                                </div>
                                <div class="col-md-10">
                                    @Html.TextAreaFor(model => model.Description, new { @class = "form-control" })
                                </div>
                            </div>
                            <!--/row-->
                            <div class="row">
                                <div class="footbar">
                                    <input type="button" id="btnReset" value="@Eagle.Resource.LanguageResource.btnReset" class="btn" />
                                    <input type="button" id="btnAdd" value="@Eagle.Resource.LanguageResource.btnSave" class="btn btn-info AddModel" />
                                </div>
                            </div>
                            <!--End.row-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
@Html.Action("jqueryval", "Common", null)

